{
    "require": {
        "twig/twig": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "App/",
            "Routes\\": "Routes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "Tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "scripts": {
        "test": [
            "phpunit"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": ""
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "license": "MIT",
    "description": "A simple PHP application using Twig for templating.",
    "keywords": [
        "php",
        "twig",
        "template"
    ],
    "homepage": "https://example.com",
    "support": {
        "blog": "https://apprendendo.blog"
    },
    "authors": [
        {
            "name": "Camila",
            "email": "milla@apprendendo.blog",
            "homepage": "https://apprendendo.blog",
            "role": "Developer"
        }
    ]
}
